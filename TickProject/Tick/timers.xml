<?xml version="1.0" encoding="utf-8"?>
<ArrayOfTimer xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Timer>
    <countdowns>
      <Countdown xsi:type="Session">
        <duration>30</duration>
      </Countdown>
      <Countdown xsi:type="ShortBreak">
        <duration>5</duration>
      </Countdown>
      <Countdown xsi:type="LongBreak">
        <duration>10</duration>
        <interval>5</interval>
        <intervalCounter>0</intervalCounter>
      </Countdown>
    </countdowns>
    <name>My Super Awesome Timer 2</name>
  </Timer>
</ArrayOfTimer>